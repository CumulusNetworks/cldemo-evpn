interfaces {
    xe-0/0/0 {
        unit 0 {
            family inet {
                address 192.168.200.22/24;
            }
        }
    }
    xe-0/0/1 {
        unit 0 {
            family inet {
                address 10.10.1.1/24;
            }
        }
    }
    xe-0/0/2 {
        unit 0 {
            family inet {
                address 10.10.2.1/24;
            }
        }
    }
    xe-0/0/3 {
        unit 0 {
            family inet {
                address 10.10.3.1/24;
            }
        }
    }
    xe-0/0/4 {
        unit 0 {
            family inet {
                address 10.10.4.1/24;
            }
        }
    }    
    em0 {
        unit 0 {
            family inet {
                dhcp;
            }
        }
    }
    em1 {
        unit 0 {
            family inet {
                address 169.254.0.2/24;
            }
        }
    }
    em5 {
        unit 0 {
            family inet {
                address 10.10.5.1/24;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address 10.0.0.22/32;
            }
        }
    }
}
routing-options {
    autonomous-system 64700;
}
protocols {
    bgp {
        traceoptions {
            file bgp.debug world-readable;
            flag update detail;
            flag general;
        }
        group FABRIC {
            type external;
            multihop {
                no-nexthop-change;
            }
            family inet {
                unicast;
            }
            family evpn {
                signaling {
                    no-install;
                }
            }
            export CUMULUS_RULES;
            local-as 64700;
            multipath multiple-as;
            neighbor 10.10.1.2 {
                description leaf01;
                peer-as 65011;
            }
            neighbor 10.10.2.2 {
                description leaf02;
                peer-as 65012;
            }            
            neighbor 10.10.3.2 {
                description leaf03;
                peer-as 65013;
            }
            neighbor 10.10.4.2 {
                description leaf04;
                peer-as 65014;
            }            
        }
    }
    lldp {
        interface all;
    }
}
policy-options {
    policy-statement CUMULUS_RULES {
        term TERM1 {
            from protocol direct;
            then accept;
        }
        term TERM2 {
            from family inet;
            then {
                next-hop self;
            }
        }
    }
}